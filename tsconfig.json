{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "lib": ["ES2015","dom"],
    "sourceMap": true, 
    "outDir": "./js",
    "strict": true,
    "strictNullChecks": true,             
    "strictFunctionTypes": true, 
    "noImplicitThis": true, 
    "alwaysStrict": true,
    "noImplicitReturns": true, 
    "esModuleInterop": true,
    "experimentalDecorators": true,        
    "forceConsistentCasingInFileNames": true 
  }
}
